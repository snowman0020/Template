{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/Template-log-.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName" ],
    "Properties": {
      "ApplicationName": "Template"
    }
  },
  "JWT": {
    "Key": "eqbqRp2AhMCy88PAVR9RYa2zjAPfwz1KD9z6eGFkOWOXustez7sUTr9CWViEY9Ig,",
    "ExpMinutes": "15",
    "Issuer": "http://localhost:5011"
  },
  "RabbitMQ": {
    "HostName": "localhost",
    "UserName": "guest",
    "Password": "guest",
    "QueueName": "template-queue"
  },
  "Redis": {
    "CacheName": "template-redis_ ",
    "InstanceName": "template-cache",
    "AbsoluteExpirationRelativeToNow": "30",
    "SlidingExpiration": "20"
  },
  "Hangfire": {
    "Username": "templateDB",
    "Password": "templateDB",
    "PrefixName": "template-db-hangfire",
    "DashboardPath": "/template-db/hangfire",
    "RedisConnectionString": "localhost:6379, ssl= False, abortConnect= False"
  },
  "CustomSetting": {
    "MainUrl": "http://localhost:5011",
    "AllowedHosts": "*",
    "DbConnectionString": "Server= (local); Database= TemplateDB; Trusted_Connection= True; TrustServerCertificate= True; Connect Timeout= 480; MultipleActiveResultSets= True;"
  }
}