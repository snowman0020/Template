{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/Template-log-.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName" ],
    "Properties": {
      "ApplicationName": "Template"
    }
  },
  "JWT": {
    "Key": "eqbqRp2AhMCy88PAVR9RYa2zjAPfwz1KD9z6eGFkOWOXustez7sUTr9CWViEY9Ig,",
    "ExpMinutes": "15",
    "Issuer": "http://localhost:5011"
  },
  "RabbitMQ": {
    "HostName": "localhost",
    "UserName": "guest",
    "Password": "guest",
    "QueueName": "template-queue"
  },
  "Redis": {
    "CacheId": "template-redis-cache-{id}",
    "InstanceName": "template-redis-cache",
    "AbsoluteExpirationRelativeToNow": "15",
    "SlidingExpiration": "10",
    "ConnectionServer": "localhost:6379"
  },
  "Hangfire": {
    "Username": "templateDB",
    "Password": "templateDB",
    "PrefixName": "template-db-hangfire",
    "DashboardPath": "/template-db/hangfire",
    "RedisConnectionServer": "localhost:6379, ssl= False, abortConnect= False"
  },
  "Email": {
    "Smtp": {
      "Server": "",
      "Port": "587",
      "Username": "",
      "Password": "",
      "PasswordEnable ": "true"
    },
    "GraphApi": {
      "TenantId": "",
      "ClientId": "",
      "ClientSecret": "",
      "PrincipalId": "",
      "PrincipalName": "",
      "Url": "https://graph.microsoft.com/.default"
    }
  },
  "CustomSetting": {
    "MainUrl": "http://localhost:5011",
    "AllowedHosts": "*",
    "ConnectionSQLServer": "Server= (local); Database= TemplateDB; User Id= sa; Password= 1234; TrustServerCertificate= True; Connect Timeout= 480; MultipleActiveResultSets= True;",
    "ConnectionMySQLServer": "Server= localhost; Database= TemplateDB; Uid= root; Pwd= root;",
    "ConnectionOracleServer": "Data Source= TemplateDB; User Id= system; Password= 1234;",
    "ConnectionSQLiteServer": "Data Source= TemplateDB.db;",
    "ConnectionPostgreSQLServer": "Server= localhost; Port= 5432; Database= TemplateDB; User Id= postgres; Password= 1234;",
    "MongoDB": {
      "ConnectionServer": "",
      "DatabaseName": "TemplateDB"
    }
  }
}